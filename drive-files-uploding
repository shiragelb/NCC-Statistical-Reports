    {
      "cell_type": "code",
      "source": [
        "from google.colab import auth\n",
        "from googleapiclient.discovery import build\n",
        "from googleapiclient.http import MediaIoBaseDownload\n",
        "from docx import Document\n",
        "import pandas as pd\n",
        "import io\n",
        "import os\n",
        "import subprocess\n",
        "\n",
        "# Authenticate\n",
        "auth.authenticate_user()\n",
        "drive_service = build('drive', 'v3')\n",
        "\n",
        "# --- Step 1: Download DOC file from Google Drive ---\n",
        "file_id = \"17MZRTxPOi_2ffyWx9tjOfDW75hF606mL\"\n",
        "local_doc_path = \"/content/temp.doc\"\n",
        "\n",
        "request = drive_service.files().get_media(fileId=file_id)\n",
        "fh = io.BytesIO()\n",
        "downloader = MediaIoBaseDownload(fh, request)\n",
        "done = False\n",
        "while not done:\n",
        "    status, done = downloader.next_chunk()\n",
        "fh.seek(0)\n",
        "\n",
        "with open(local_doc_path, \"wb\") as f:\n",
        "    f.write(fh.read())\n",
        "\n",
        "print(\"Downloaded DOC file:\", local_doc_path)\n",
        "\n",
        "# --- Step 2: Convert DOC to DOCX ---\n",
        "docx_path = local_doc_path.replace(\".doc\", \".docx\")\n",
        "subprocess.run([\n",
        "    \"libreoffice\", \"--headless\", \"--convert-to\", \"docx\", local_doc_path, \"--outdir\", os.path.dirname(local_doc_path)\n",
        "], check=True)\n",
        "\n",
        "print(\"Converted to DOCX:\", docx_path)\n",
        "\n",
        "# --- Step 3: Extract tables from DOCX ---\n",
        "doc = Document(docx_path)\n",
        "all_rows = []\n",
        "\n",
        "for i, table in enumerate(doc.tables):\n",
        "    table_name = f\"Table_{i+1}\"\n",
        "    # If first row has single cell, treat it as table title\n",
        "    first_row = [cell.text.strip() for cell in table.rows[0].cells]\n",
        "    start_row = 0\n",
        "    if len(first_row) == 1 and len(table.rows) > 1:\n",
        "        table_name = first_row[0] if first_row[0] else table_name\n",
        "        start_row = 1\n",
        "\n",
        "    for row_idx, row in enumerate(table.rows[start_row:], start=start_row+1):\n",
        "        row_data = [cell.text.strip() for cell in row.cells]\n",
        "        all_rows.append({\n",
        "            \"table_name\": table_name,\n",
        "            \"row_number\": row_idx,\n",
        "            \"cell_data\": row_data\n",
        "        })\n",
        "\n",
        "# --- Step 4: Save to DataFrame ---\n",
        "tables_df = pd.DataFrame(all_rows)\n",
        "print(\"Extracted tables:\")\n",
        "display(tables_df)\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 141
