        "file_id = \"17MZRTxPOi_2ffyWx9tjOfDW75hF606mL\"\n",
        "\n",
        "# ×”×•×¨×“×ª ×”×§×•×‘×¥ ×œ-Colab\n",
        "local_path = \"test.doc\"  # ×©× ×–×× ×™\n",
        "request = drive_service.files().get_media(fileId=file_id)\n",
        "fh = io.BytesIO()\n",
        "downloader = MediaIoBaseDownload(fh, request)\n",
