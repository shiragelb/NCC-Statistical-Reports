      "cell_type": "code",
      "source": [
        "\n",
        "import textract\n",
        "import pandas as pd\n",
        "\n",
        "def extract_tables_from_doc(doc_path):\n",
        "    \"\"\"Extract tables from DOC (text-based approach)\"\"\"\n",
        "    # Extract text from DOC\n",
        "    text = textract.process(doc_path).decode(\"utf-8\")\n",
        "\n",
        "    tables = []\n",
        "\n",
        "    # Split the text into blocks separated by two newlines (× ×™×¡×™×•×Ÿ ×œ×”×¤×¨×™×“ ×˜×‘×œ××•×ª)\n",
        "    blocks = text.split(\"\\n\\n\")\n",
        "\n",
        "    for i, block in enumerate(blocks):\n",
        "        lines = [line for line in block.splitlines() if line.strip() != \"\"]\n",
        "\n",
        "        # × × ×™×— ×©×˜×‘×œ×” ×”×™× ×‘×œ×•×§ ×©×‘×• ×œ×¤×—×•×ª 2 ×©×•×¨×•×ª ×•×™×© ×˜××‘×™× ××• ×¨×•×•×—×™× ××•×¤×¨×“×™×\n",
        "        if len(lines) >= 2 and any(\"\\t\" in line or \" \" in line for line in lines):\n",
        "            data = [line.split(\"\\t\") if \"\\t\" in line else line.split() for line in lines]\n",
        "\n",
        "            df = pd.DataFrame(data)\n",
        "\n",
        "            # ×©×™××•×© ×‘×©×•×¨×” ×”×¨××©×•× ×” ×›×›×•×ª×¨×ª ×× ×”×™× × ×¨××™×ª ×›××• ×›×•×ª×¨×ª (×©×•×¨×” ×¢× ×ª× ××—×“ ×‘×œ×‘×“)\n",
        "            table_name = f\"Table_{i+1}\"\n",
        "            if len(data[0]) == 1 and len(data) > 1:\n",
        "                table_name = data[0][0]\n",
        "                df = pd.DataFrame(data[1:])\n",
        "\n",
        "            tables.append((table_name, df))\n",
        "\n",
        "    return tables\n"
      ],
