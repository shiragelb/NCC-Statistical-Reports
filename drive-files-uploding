        "from google.colab import auth\n",
        "import googleapiclient.http\n",
        "from googleapiclient.discovery import build\n",
        "import pandas as pd\n",
        "\n",
        "# Authenticate\n",
        "auth.authenticate_user()\n",
        "\n",
        "# Build Drive API service\n",
        "drive_service = build('drive', 'v3')\n",
        "\n",
        "# File ID (from your link)\n",
        "file_id = \"17MZRTxPOi_2ffyWx9tjOfDW75hF606mL\"\n",
        "\n",
        "# Download file content as Excel\n",
        "request = drive_service.files().get_media(fileId=file_id)\n",
        "fh = open(\"file.xlsx\", \"wb\")\n",
        "downloader = googleapiclient.http.MediaIoBaseDownload(fh, request)\n",
        "\n",
        "done = False\n",
        "while not done:\n",
        "    status, done = downloader.next_chunk()\n",
        "\n",
        "fh.close()\n",
        "\n",
        "# Read Excel into DataFrame\n",
        "df = pd.read_excel(\"file.xlsx\")"
      ],
      "metadata": {
        "id": "JFrx6rWlvQwu"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "from google.colab import auth\n",
        "from googleapiclient.discovery import build\n",
        "from googleapiclient.http import MediaIoBaseDownload\n",
        "import pandas as pd\n",
        "from docx import Document\n",
        "import io\n",
        "import os\n",
        "import subprocess\n",
        "\n",
        "# Authenticate\n",
        "auth.authenticate_user()\n",
        "\n",
        "# Build Drive API service\n",
        "drive_service = build('drive', 'v3')\n",
        "\n",
        "def download_drive_file(file_id, local_path):\n",
        "    \"\"\"Download file from Google Drive to local path\"\"\"\n",
        "    request = drive_service.files().get_media(fileId=file_id)\n",
        "    fh = io.BytesIO()\n",
        "    downloader = MediaIoBaseDownload(fh, request)\n",
        "    done = False\n",
        "    while not done:\n",
        "        status, done = downloader.next_chunk()\n",
        "    fh.seek(0)\n",
        "    with open(local_path, \"wb\") as f:\n",
        "        f.write(fh.read())\n",
        "    return local_path\n",
        "\n",
        "def convert_doc_to_docx(doc_path):\n",
        "    \"\"\"Convert .doc to .docx using LibreOffice\"\"\"\n",
        "    if doc_path.lower().endswith(\".doc\"):\n",
        "        docx_path = doc_path.replace(\".doc\", \".docx\")\n",
        "        subprocess.run([\n",
        "            \"libreoffice\", \"--headless\", \"--convert-to\", \"docx\", doc_path, \"--outdir\", os.path.dirname(doc_path)\n",
        "        ], check=True)\n",
        "        return docx_path\n",
        "    return doc_path\n",
        "\n",
        "def extract_tables_from_docx(docx_path):\n",
        "    \"\"\"Extract tables from DOCX file\"\"\"\n",
        "    doc = Document(docx_path)\n",
        "    tables_list = []\n",
        "    for i, table in enumerate(doc.tables):\n",
        "        data = [[cell.text.strip() for cell in row.cells] for row in table.rows]\n",
        "        if data:\n",
        "            df_table = pd.DataFrame(data)\n",
        "            table_name = f\"Table_{i+1}\"\n",
        "            # use first row as table name if it has single cell\n",
        "            if len(data[0]) == 1 and len(data) > 1:\n",
        "                table_name = data[0][0] if data[0][0] else table_name\n",
        "                df_table = pd.DataFrame(data[1:])\n",
        "            tables_list.append((table_name, df_table))\n",
        "    return tables_list\n",
        "\n",
        "# Example usage:\n",
        "file_id = \"17MZRTxPOi_2ffyWx9tjOfDW75hF606mL\"\n",
        "local_file = download_drive_file(file_id, \"temp.doc\")  # download from Drive\n",
        "docx_file = convert_doc_to_docx(local_file)            # convert if needed\n",
        "tables = extract_tables_from_docx(docx_file)           # extract tables\n",
        "\n",
        "# Print results\n",
        "for name, df_table in tables:\n",
        "    print(\"Table name:\", name)\n",
        "    print(df_table)\n"
      ],
      "metadata": {
        "id": "NZeAi-f7v4GC"
      },
      "execution_count": 31,
      "outputs": []
    },
