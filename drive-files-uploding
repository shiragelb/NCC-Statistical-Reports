        "# ================================\n",
        "# ×©×œ×‘ 1: ×”×ª×§× ×ª ×¡×¤×¨×™×™×ª google-api-python-client\n",
        "# ================================\n",
        "!pip install -q google-api-python-client google-auth-httplib2 google-auth-oauthlib\n",
        "\n",
        "# ================================\n",
        "# ×©×œ×‘ 2: ×”×ª×—×‘×¨×•×ª ×œ-Google Drive API\n",
        "# ================================\n",
        "from google.colab import auth\n",
        "auth.authenticate_user()\n",
        "\n",
        "from googleapiclient.discovery import build\n",
        "drive_service = build('drive', 'v3')\n",
        "\n",
        "# ================================\n",
        "# ×©×œ×‘ 3: ×”×’×“×¨×ª folder_id\n",
        "# ================================\n",
        "# ××–×”×” ×”×ª×™×§×™×™×” ×‘×“×¨×™×™×‘ (×”×—×œ×§ ××—×¨×™ 'folders/' ×‘-URL)\n",
        "folder_id = \"1VHokeD7hf3vZq_UBIVOzFWJeEoCcrApv\"\n",
        "\n",
        "# ================================\n",
        "# ×©×œ×‘ 4: ×©×œ×™×¤×ª ×§×‘×¦×™× ×¢× ×¡×™×•××•×ª docx ××• pdf\n",
        "# ================================\n",
        "query = f\"'{folder_id}' in parents and trashed=false and (mimeType='application/vnd.openxmlformats-officedocument.wordprocessingml.document' or mimeType='application/pdf')\"\n",
        "\n",
        "results = drive_service.files().list(q=query, pageSize=1000, fields=\"files(id, name, mimeType)\").execute()\n",
        "files = results.get('files', [])\n",
        "\n",
        "# ================================\n",
        "# ×©×œ×‘ 5: ×”×•×¨×“×” ×–×× ×™×ª ×©×œ ×”×§×‘×¦×™× ×œ-Colab\n",
        "# ================================\n",
        "import pandas as pd\n",
        "import io\n",
        "from googleapiclient.http import MediaIoBaseDownload\n",
        "\n",
        "download_path = \"/content/downloaded_files\"\n",
        "import os\n",
        "os.makedirs(download_path, exist_ok=True)\n",
        "\n",
        "file_records = []\n",
        "\n",
        "for file in files:\n",
        "    file_id = file['id']\n",
        "    file_name = file['name']\n",
        "    mime_type = file['mimeType']\n",
        "\n",
        "    # ×”×•×¨×“×” ×–×× ×™×ª\n",
        "    request = drive_service.files().get_media(fileId=file_id)\n",
        "    fh = io.FileIO(os.path.join(download_path, file_name), 'wb')\n",
        "    downloader = MediaIoBaseDownload(fh, request)\n",
        "    done = False\n",
        "    while not done:\n",
        "        status, done = downloader.next_chunk()\n",
        "\n",
        "    # ×©××™×¨×” ×‘××™×œ×•×Ÿ ×œ×¨×©×•××” ×‘-DataFrame\n",
        "    file_records.append({\n",
        "        \"file_name\": file_name,\n",
        "        \"mime_type\": mime_type,\n",
        "        \"local_path\": os.path.join(download_path, file_name)\n",
        "    })\n",
        "    print(\"ğŸ“‚ ×”×•×¨×“:\", file_name)\n",
        "\n",
        "# ================================\n",
        "# ×©×œ×‘ 6: ×™×¦×™×¨×ª DataFrame\n",
        "# ================================\n",
        "df_files = pd.DataFrame(file_records)\n",
        "print(\"âœ… ×›×œ ×”×§×‘×¦×™× ×–××™× ×™× ×‘-DataFrame:\")\n",
        "df_files\n"
