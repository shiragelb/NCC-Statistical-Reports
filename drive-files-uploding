      "cell_type": "code",
      "source": [
        "import os\n",
        "import subprocess\n",
        "import pandas as pd\n",
        "from docx import Document\n",
        "\n",
        "def extract_tables_with_names(doc_path):\n",
        "    \"\"\"\n",
        "    Extract tables with their names from a Word document.\n",
        "    Supports both .doc and .docx: .doc files are converted to .docx first.\n",
        "    \"\"\"\n",
        "    # ×× ×”×§×•×‘×¥ ×”×•× .doc (×•×œ× docx), × ××™×¨ ××•×ª×•\n",
        "    if doc_path.lower().endswith(\".doc\"):\n",
        "        docx_path = doc_path.replace(\".doc\", \".docx\")\n",
        "        # ×”××¨×” ×‘×××¦×¢×•×ª LibreOffice (×¢×•×‘×“ ×‘-Colab)\n",
        "        subprocess.run([\n",
        "            \"libreoffice\", \"--headless\", \"--convert-to\", \"docx\", doc_path, \"--outdir\", os.path.dirname(doc_path)\n",
        "        ], check=True)\n",
        "    else:\n",
        "        docx_path = doc_path\n",
        "\n",
        "    # ×§×¨×™××” ×©×œ ×§×•×‘×¥ ×”-DOCX\n",
        "    doc = Document(docx_path)\n",
        "    tables = []\n",
        "\n",
        "    for i, table in enumerate(doc.tables):\n",
        "        # ×—×™×œ×•×¥ ×ª×•×›×Ÿ ×”×˜×‘×œ×”\n",
        "        data = []\n",
        "        for row in table.rows:\n",
        "            data.append([cell.text.strip() for cell in row.cells])\n",
        "\n",
        "        if data:\n",
        "            df = pd.DataFrame(data)\n",
        "\n",
        "            # × × ×¡×” ×œ××¦×•× ×©× ×œ×˜×‘×œ×” ××”×©×•×¨×” ×”×¨××©×•× ×” ××• × ×©×ª××© ×‘×©× ×‘×¨×™×¨×ª ××—×“×œ\n",
        "            table_name = f\"Table_{i+1}\"  # ×‘×¨×™×¨×ª ××—×“×œ\n",
        "            if len(data[0]) == 1 and len(data) > 1:  # ×ª× ××—×“ ×‘×©×•×¨×” ×”×¨××©×•× ×”? × × ×™×— ×©×–×” ×›×•×ª×¨×ª\n",
        "                table_name = data[0][0] if data[0][0] else table_name\n",
        "                df = pd.DataFrame(data[1:])  # ×“×™×œ×•×’ ×¢×œ ×©×•×¨×ª ×”×›×•×ª×¨×ª\n",
        "\n",
        "            tables.append((table_name, df))\n",
        "\n",
        "    return tables\n"
      ],
      "metadata": {
        "id": "QilFZry1sJ-x"
      },
