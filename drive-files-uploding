          "execution_count": 42
        }
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "from google.colab import auth\n",
        "from googleapiclient.discovery import build\n",
        "from googleapiclient.http import MediaIoBaseDownload\n",
        "from docx import Document\n",
        "import pandas as pd\n",
        "import io\n",
        "import os\n",
        "\n",
        "# --- Authenticate ---\n",
        "auth.authenticate_user()\n",
        "drive_service = build('drive', 'v3')\n",
        "\n",
        "# --- File ID of the DOCX ---\n",
        "file_id = \"1X7bK7gl9UibMO0iLj0qxMX7d0nX0PJO-\"\n",
        "local_path = \"/content/temp.docx\"\n",
        "\n",
        "# --- Step 1: Download the DOCX file ---\n",
        "request = drive_service.files().get_media(fileId=file_id)\n",
        "fh = io.BytesIO()\n",
        "downloader = MediaIoBaseDownload(fh, request)\n",
        "done = False\n",
        "while not done:\n",
        "    status, done = downloader.next_chunk()\n",
        "fh.seek(0)\n",
        "\n",
        "with open(local_path, \"wb\") as f:\n",
        "    f.write(fh.read())\n",
        "\n",
        "print(\"Downloaded DOCX file:\", local_path)\n",
        "print(\"File size (bytes):\", os.path.getsize(local_path))\n",
        "\n",
        "# --- Step 2: Extract tables from DOCX ---\n",
        "def extract_tables_with_names(docx_path):\n",
        "    doc = Document(docx_path)\n",
        "    tables_list = []\n",
        "\n",
        "    for i, table in enumerate(doc.tables):\n",
        "        # Extract table data\n",
        "        data = [[cell.text.strip() for cell in row.cells] for row in table.rows]\n",
        "\n",
        "        if data:\n",
        "            df_table = pd.DataFrame(data)\n",
        "            table_name = f\"Table_{i+1}\"  # Default name\n",
        "\n",
        "            # If first row has single cell, treat as table title\n",
        "            if len(data[0]) == 1 and len(data) > 1:\n",
        "                table_name = data[0][0] if data[0][0] else table_name\n",
        "                df_table = pd.DataFrame(data[1:])  # Skip title row\n",
        "\n",
        "            tables_list.append((table_name, df_table))\n",
        "\n",
        "    return tables_list\n",
        "\n",
        "# --- Step 3: Apply function to the downloaded file ---\n",
        "tables = extract_tables_with_names(local_path)\n",
        "\n",
        "# --- Step 4: Convert tables to a single DataFrame ---\n",
        "all_rows = []\n",
        "for table_name, df_table in tables:\n",
        "    for row_idx, row in df_table.iterrows():\n",
        "        all_rows.append({\n",
        "            \"table_name\": table_name,\n",
        "            \"row_number\": row_idx + 1,\n",
        "            \"cell_data\": row.tolist()\n",
        "        })\n",
        "\n",
        "final_df = pd.DataFrame(all_rows)\n",
        "print(\"Extracted tables from DOCX:\")\n",
        "display(final_df)\n"
      ],
